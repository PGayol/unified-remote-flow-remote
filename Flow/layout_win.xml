<?xml version="1.0" encoding="utf-8"?>
<layout onLaunch="launch" onVolumeUp="volume_up" onVolumeDown="volume_down" onResume="resume" onPause="pause" color="#29f0b7">
  <grid>
    <row>
      <button icon="rwd" onTap="rewind" />
      <button icon="select" onTap="select" />
      <button icon="ff" onTap="forward" />
    </row>
    <row>
      <button icon="vup" onTap="volume_up" />
      <button icon="playpause" onTap="play_pause" />
      <button icon="up" onTap="right" />
    </row>
    <row>
      <button icon="vdown" onTap="volume_down" />
      <button icon="vmute" onTap="volume_mute" />
      <button icon="down" onTap="left" />
    </row>
    <row>
      <button icon="fullscreen" onTap="fullscreen" />
    </row>
  </grid>
</layout>